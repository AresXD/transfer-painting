{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential", "standard"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["vue"],
  "rules": {
    "camelcase": "off",
    "space-before-function-paren": "off",
    "no-multi-str": "off",
    "no-unused-vars": "off"
  },
  "globals": {
    "OriginalPalette": "readonly",
    "ReferralPalette": "readonly",
    "OriginalColor": "readonly",
    "ReferralColor": "readonly",
    "math": "readonly",
    "original_color_list_rgb": "writable",
    "original_color_list1": "writable",
    "original_color_list2": "writable",
    "original_change_list": "readonly",
    "original_Ls": "readonly",
    "original_Ls_user": "readonly",
    "referral_color_list_rgb": "writable",
    "referral_color_list1": "writable",
    "referral_color_list2": "writable",
    "referral_change_list": "readonly",
    "referral_Ls": "readonly",
    "referral_Ls_user": "readonly",
    "$": "readonly"
  }
}
